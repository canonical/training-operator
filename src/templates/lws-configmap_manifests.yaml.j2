apiVersion: v1
data:
  controller_manager_config.yaml: |
    apiVersion: config.lws.x-k8s.io/v1alpha1
    kind: Configuration
    leaderElection:
      leaderElect: true
    internalCertManagement:
      enable: true
      webhookServiceName: {{ app_name }}-lws-controller-manager
      webhookSecretName: {{ app_name }}-lws-webhook-server-cert
kind: ConfigMap
metadata:
  name: {{ app_name }}-lws-manager-config
  namespace: {{ namespace }}
