alert: TrainingOperatorUnitIsUnavailable
expr: up < 1
for: 5m
labels:
  severity: critical
annotations:
  summary: Training-operator unit {{ $labels.juju_model }}/{{ $labels.juju_unit }} unavailable
  description: >
    The Training-operator unit {{ $labels.juju_model }} {{ $labels.juju_unit }} is unavailable
    LABELS = {{ $labels }}
